<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox accessibleText="text" prefHeight="105.0" prefWidth="170.0" spacing="10.0" style="-fx-background-color: #43545e;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="menu.quitController">
   <children>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/icons8-export-26.png" />
               </image>
               <HBox.margin>
                  <Insets left="10.0" top="10.0" />
               </HBox.margin>
            </ImageView>
            <Button mnemonicParsing="false" onAction="#logoutClicked" prefHeight="50.0" prefWidth="170.0" styleClass="buttonToQuit" stylesheets="@/styles/styles.css" text="Deconnecter" textFill="WHITE" textOverrun="CLIP">
               <font>
                  <Font name="Arial Bold" size="15.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/icons8-power-off-button-32.png" />
               </image>
               <HBox.margin>
                  <Insets left="7.0" top="7.0" />
               </HBox.margin>
            </ImageView>
            <Button mnemonicParsing="false" onAction="#quitClicked" prefHeight="46.0" prefWidth="133.0" styleClass="buttonToQuit" stylesheets="@/styles/styles.css" text="Quiter" textFill="WHITE" textOverrun="CLIP">
               <font>
                  <Font name="Arial Bold" size="15.0" />
               </font>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
